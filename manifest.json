{
  "name": "Kittens Everywhere",
  "version": "1.0",
  "description": "Kittens Everywhere turns every image you see into an adorable kitten picture. (Toggleable)",
  "background_page": "background.html",
  "permissions": [
    "tabs", "http://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["jquery.js", "placekittens.js"]
    }
  ],
  "browser_action": {
      "default_title": "Kitten mode: OFF",
      "default_icon": "iconbw21.jpg"
  },
  "icons": { "16": "icon16.jpg",
             "48": "icon48.jpg",
             "128": "icon128.jpg" }
}